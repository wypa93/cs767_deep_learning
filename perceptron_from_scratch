        "## 1. Data Load and Preparation"
      ],
      "metadata": {
        "id": "bGilTwUVu9Hu"
      }
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "kZcETcpLGmpz"
      },
      "outputs": [],
      "source": [
        "#load libraries\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import plotly.express as px\n",
        "from sklearn import datasets\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sympy import symbols, diff\n",
        "\n",
        "#prepare data\n",
        "iris = datasets.load_iris()\n",
        "y = np.where(iris.target > 1,1,iris.target)\n",
        "X = iris.data\n",
        "X_train, X_test, y_train, y_test = train_test_split(X,y,train_size=0.8,random_state=10)"
      ]
    },
    {
      "cell_type": "markdown",
      "source": [
        "## 2. Implementation as Single Perceptron-Class"
      ],
      "metadata": {
        "id": "fsiBNLVj9shN"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "class Perceptron():\n",
        "\n",
        "  def __init__(self,X,y,X_test,y_test,lRate,n_epochs): #instantiate Object\n",
        "      self.weights = np.random.rand(1,X.shape[1]).ravel()\n",
        "      self. bias = np.random.rand(1,1).ravel()\n",
        "      self.X = X\n",
        "      self.y = y\n",
        "      self.X_test = X_test\n",
        "      self.y_test = y_test\n",
        "      self.lRate = lRate\n",
        "      self.n_epochs = n_epochs\n",
        "      self.error = 0\n",
        "      self.mse = []\n",
        "      self.epochs = []\n",
        "\n",
        "  def forwardProp(self): #calculate error yhat using forward propagation\n",
        "      yhat = 1/(1+np.exp(-(np.dot(self.weights,self.X.T)+self.bias))) \n",
        "      self.error = self.y - yhat\n",
        "\n",
        "  def backwardProp(self): #calculate gradients and update weights/bias\n",
        "      self.weights -= -(2/len(X))*(np.dot(self.X.T,self.error)) * self.lRate\n",
        "      self.bias -= -(2/len(X))*np.sum(self.error) * self.lRate\n",
        "\n",
        "  def trainBatchGD(self): #train Perceptron using Batch Gradient Descent\n",
        "    for epoch in range(self.n_epochs):\n",
        "      self.forwardProp()\n",
        "      self.backwardProp()\n",
        "      self.epochs.append(epoch)\n",
        "      self.mse.append(np.mean(self.error**2))\n",
        "      if epoch % 1000 == 0:\n",
        "        print('Batch GD Epoch:',epoch,'Mean Squared Error:',round(np.mean(self.error**2),3))\n",
        "    return plt.plot(self.mse)\n",
        "\n",
        "  def trainSGD(self): #train Perceptron using Stochastic Gradient Descent\n",
        "    for epoch in range(self.n_epochs):\n",
        "      rand_idx = np.random.randint(self.X.shape[0]) #select random index for subset X,y\n",
        "      self.X=self.X[rand_idx:rand_idx+1] \n",
        "      self.y=self.y[rand_idx:rand_idx+1]\n",
        "      self.forwardProp()\n",
        "      self.backwardProp()\n",
        "      self.epochs.append(epoch)\n",
        "      self.mse.append(np.mean(self.error**2))\n",
        "      if epoch % 1000 == 0:\n",
        "        print('SGD Epoch:',epoch,'Mean Squared Error:',round(np.mean(self.error**2),3))\n",
        "    return plt.plot(self.mse)\n",
        "\n",
        "  def trainMiniBatchGD(self): #train Perceptron using Stochastic Gradient Descent\n",
        "    for epoch in range(self.n_epochs):\n",
        "      rand_idx = np.random.randint(self.X.shape[0],size=12) #randomly select index to assemble mini batch of size 10\n",
        "      self.X=self.X[rand_idx,:]\n",
        "      self.y=np.take(self.y,rand_idx)\n",
        "      self.forwardProp()\n",
        "      self.backwardProp()\n",
        "      self.epochs.append(epoch)\n",
        "      self.mse.append(np.mean(self.error**2))\n",
        "      if epoch % 1000 == 0:\n",
        "        print('mini Batch GD Epoch:',epoch,'Mean Squared Error:',round(np.mean(self.error**2),3))\n",
        "    return plt.plot(self.mse)\n",
        "\n",
        "  def test(self):\n",
        "    yhat = 1/(1+np.exp(-(np.dot(self.weights,self.X_test.T)+self.bias)))\n",
        "    return plt.plot((self.y_test - yhat)**2)"
      ],
      "metadata": {
        "id": "N0nfOyqTDGgc"
      },
      "execution_count": null,
      "outputs": []
    },
